# gitignore template for Drupal 8 projects
#
# earlier versions of Drupal are tracked in `community/Python/`

# Ignore configuration files that may contain sensitive information
/web/sites/*/*settings*.php
/web/sites/*/*services*.yml

# Ignore paths that may contain user-generated content
/sites/*/files
/sites/*/public
/sites/*/private
/sites/*/files-public
/sites/*/files-private

# Ignore paths that may contain temporary files
/sites/*/translations
/sites/*/tmp
/sites/*/cache

# Ignore drupal core (if not versioning drupal sources)
/web/core
/web/modules/README.txt
/web/profiles/README.txt
/web/sites/README.txt
/web/sites/example.sites.php
/web/sites/example.settings.local.php
/web/sites/development.services.yml
/web/themes/README.
/web/themes/contrib
/web/modules/contrib
/vendor
/web/.csslintrc
/web/.editorconfig
/web/.eslintignore
/web/.eslintrc.json
/web/.gitattributes
/web/.htaccess
/web/autoload.php
/web/example.gitignore
/web/index.php
/web/LICENSE.txt
/web/README.txt
/web/robots.txt
/web/update.php
/web/web.config
# Ignore development and testing files
/web/tests
/web/tests/behat
/web/tests/fixtures
/web/tests/legacy
/web/tests/legacy/core
/web/tests/legacy/modules
/web/tests/legacy/profiles
/web/tests/legacy/themes
/web/tests/legacy/vendor
# Ignore files generated by Drupal Console
/web/drupal
# Ignore files generated by Drush
/web/drush
# Ignore files generated by Composer
/web/vendor
# Ignore files generated by PHPStorm
/web/.idea
# Ignore files generated by PHP CodeSniffer
/web/phpcs.xml
# Ignore files generated by PHPStan
/web/phpstan.neon
# Ignore files generated by PHPUnit
/web/phpunit.xml
# Ignore files generated by Behat
/web/behat.yml
# Ignore files generated by Drupal Code Generator
/web/drupal-code-generator
# Ignore files generated by Drupal Coder
/web/coder
# Ignore files generated by Drupal Rector
/web/rector
# Ignore files generated by Drupal Module Upgrader
/web/drupal-module-upgrader
# Ignore files generated by Drupal Module Builder
/web/drupal-module-builder
# Ignore files generated by Drupal Theme Builder
/web/drupal-theme-builder
# Ignore files generated by Drupal Console
/web/drupal-console
# Ignore files generated by Drupal Composer
/web/drupal-composer
# Ignore files generated by Drupal Git
/drupal-git
# Ignore files generated by Drupal Git Hooks
/drupal-git-hooks
# Ignore files generated by Drupal Git Ignore
/drupal-git-ignore
# Ignore files generated by Drupal Git LFS
/drupal-git-lfs
# Ignore files generated by Drupal Git Flow
/drupal-git-flow
# Ignore files generated by Drupal Git Submodules
/drupal-git-submodules
# Ignore files generated by Drupal Git Tags
/drupal-git-tags
# Ignore files generated by Drupal Git Branches
/drupal-git-branches
# Ignore files generated by Drupal Git Commits
/drupal-git-commits
# Ignore files generated by Drupal Git Repositories
/drupal-git-repositories
# Ignore files generated by Drupal Git Remotes
/drupal-git-remotes
# Ignore files generated by Drupal Git Stashes
/drupal-git-stashes
# Ignore files generated by Drupal Git Worktrees
/drupal-git-worktrees